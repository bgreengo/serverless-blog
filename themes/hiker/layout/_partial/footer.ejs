<footer id="footer">
  <% if (theme.sidebar === 'bottom'){ %>
    <%- partial('_partial/sidebar') %>
  <% } %>

  <div class="container">
      	<div class="row">
	      <p id="copyRightEn">Copyright &copy; <%= theme.since ||  (date(new Date(), 'YYYY')-1)%> - <%= date(new Date(), 'YYYY') %> <%= config.title || config.author %> All Rights Reserved.</p>
	    
		</div>

		
  </div>
  <form
    action="https://buttondown.email/api/emails/embed-subscribe/greengocloud"
    method="post"
    target="popupwindow"
    onsubmit="window.open('https://buttondown.email/greengocloud', 'popupwindow')"
    class="embeddable-buttondown-form"
  >
    <label for="bd-email">greengocloud newsletter, enter your email</label>
    <input type="email" name="email" id="bd-email">
    <input type="hidden" value="1" name="embed"/>
    <input type="submit" value="Subscribe" />
    <p>
      <a href="https://buttondown.email/greengocloud" target="_blank">Powered by GREENGOCLOUD</a>
    </p>
  </form>
</footer>


<!-- min height -->

<script>
    var wrapdiv = document.getElementById("wrap");
    var contentdiv = document.getElementById("content");
    var allheader = document.getElementById("allheader");

    wrapdiv.style.minHeight = document.body.offsetHeight + "px";
    if (allheader != null) {
      contentdiv.style.minHeight = document.body.offsetHeight - allheader.offsetHeight - document.getElementById("footer").offsetHeight + "px";
    } else {
      contentdiv.style.minHeight = document.body.offsetHeight - document.getElementById("footer").offsetHeight + "px";
    }
</script>